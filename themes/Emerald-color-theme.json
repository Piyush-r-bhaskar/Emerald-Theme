	"tokenColors": [
		{
			"scope": [],
			"settings": {
				"foreground": "#a2aabc"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0b942998"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
