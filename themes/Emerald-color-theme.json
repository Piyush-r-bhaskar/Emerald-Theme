				"punctuation.definition.entity",
				"constant.character.entity",
				"punctuation.definition.template-expression",
				"punctuation.section.tag.twig",
			],
			"settings": {
				"foreground": "#b793ff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#c94873"
			}
		},
